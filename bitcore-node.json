{
  "network": "testnet",
  "port": 3001,
  "services": [
    "insight-api",
    "bitcoind",
    "web"
  ],
  "servicesConfig": {
    "bitcoind": {
      "spawn": {
        "datadir": "/bitcore/",
        "exec": "/bitcore/bitcoin/src/bitcoind"
      }
    },
    "insight-api": {
      "routePrefix": "api",
      "enableCache": true
    }
  }
}
